{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "definitions": {
        "GlueDataSetExtension": {
            "type": "object",
            "properties": {
                "database_name": {
                    "type": "string"
                },
                "create_time": {
                    "type": "string",
                    "format": "date-time"
                },
                "last_access_time": {
                    "type": "string",
                    "format": "date-time"
                },
                "retention": {
                    "type": "string"
                },
                "storage_descriptor": {
                    "$ref": "#/definitions/GlueStorageDescriptor"
                },
                "parameters": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "partition_keys": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "table_type": {
                    "type": "string"
                },
                "created_by": {
                    "type": "string"
                },
                "is_registered_with_lake_formation": {
                    "type": "boolean"
                },
                "catalog_id": {
                    "type": "string"
                }
            }
        },
        "GlueStorageDescriptor": {
            "type": "object",
            "properties": {
                "location": {
                    "type": "string"
                },
                "input_format": {
                    "type": "string"
                },
                "output_format": {
                    "type": "string"
                },
                "compressed": {
                    "type": "string"
                },
                "number_of_buckets": {
                    "type": "integer"
                },
                "bucket_columns": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "sort_columns": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "stored_as_sub_directories": {
                    "type": "boolean"
                }
            }
        },
        "GlueDataTransformerExtension": {
            "type": "object",
            "properties": {
                "role": {
                    "type": "string"
                },
                "created_on": {
                    "type": "string",
                    "format": "date-time"
                },
                "last_modified_on": {
                    "type": "string",
                    "format": "date-time"
                },
                "max_retries": {
                    "type": "integer"
                },
                "max_capacity": {
                    "type": "number"
                },
                "worker_type": {
                    "type": "string"
                },
                "number_of_workers": {
                    "type": "integer"
                },
                "glue_version": {
                    "type": "string"
                }
            }
        },
        "GlueDataTransformerRunExtension": {
            "type": "object",
            "properties": {
                "attempt": {
                    "type": "integer"
                },
                "previous_run_id": {
                    "type": "string"
                },
                "trigger_name": {
                    "type": "string"
                },
                "last_modified_on": {
                    "type": "string",
                    "format": "date-time"
                },
                "execution_time": {
                    "type": "integer"
                },
                "timeout": {
                    "type": "integer"
                },
                "max_capacity": {
                    "type": "number"
                },
                "log_group_name": {
                    "type": "string"
                },
                "worker_type": {
                    "type": "string"
                },
                "number_of_workers": {
                    "type": "integer"
                },
                "glue_version": {
                    "type": "string"
                }
            }
        }
    }
}
